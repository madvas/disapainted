{"pkg"               : {
  "name"            : "meanjs",
  "description"     : "Full-Stack JavaScript with MongoDB, Express, AngularJS, and Node.js.",
  "version"         : "0.3.3",
  "private"         : false,
  "author"          : "https://github.com/meanjs/mean/graphs/contributors",
  "repository"      : {
    "type" : "git",
    "url"  : "https://github.com/meanjs/mean.git"
  },
  "engines"         : {
    "node" : "0.10.x",
    "npm"  : "1.4.x"
  },
  "scripts"         : {
    "start"       : "grunt",
    "test"        : "grunt test",
    "postinstall" : "bower install --config.interactive=false"
  },
  "dependencies"    : {
    "async"                     : "^0.9.0",
    "base64resize"              : "^1.0.0",
    "body-parser"               : "~1.9.0",
    "bower"                     : "~1.3.8",
    "chalk"                     : "~0.5",
    "compression"               : "~1.1.0",
    "connect-flash"             : "~0.1.1",
    "connect-mongo"             : "~0.4.1",
    "consolidate"               : "~0.10.0",
    "cookie-parser"             : "~1.3.2",
    "express"                   : "~4.9.5",
    "express-session"           : "~1.8.2",
    "express-validator"         : "^2.7.0",
    "forever"                   : "~0.11.0",
    "glob"                      : "~4.0.5",
    "grunt-cli"                 : "~0.1.13",
    "grunt-contrib-less"        : "^0.12.0",
    "helmet"                    : "~0.4.0",
    "jsonpack"                  : "^1.1.2",
    "lodash"                    : "^2.4.1",
    "method-override"           : "~2.2.0",
    "mock-nodemailer"           : "0.0.1",
    "moment"                    : "^2.8.3",
    "mongoose"                  : "~3.8.8",
    "mongoose-paginate"         : "^3.1.3",
    "mongoose-rattle-plugin"    : "^0.4.2",
    "mongoose-simple-random"    : "^0.3.0",
    "mongoose-unique-validator" : "^0.4.1",
    "mongoose-validator"        : "^1.0.2",
    "morgan"                    : "~1.3.2",
    "node-sprite-generator"     : "^0.7.0",
    "nodemailer"                : "~1.3.0",
    "paginate"                  : "^0.1.1",
    "paper"                     : "^0.9.21",
    "passport"                  : "~0.2.0",
    "passport-facebook"         : "~1.0.2",
    "passport-github"           : "~0.1.5",
    "passport-google-oauth"     : "~0.1.5",
    "passport-linkedin"         : "~0.1.3",
    "passport-local"            : "~1.0.0",
    "passport-twitter"          : "~1.0.2",
    "randomstring"              : "^1.0.3",
    "sinon"                     : "^1.10.3",
    "swig"                      : "~1.4.1"
  },
  "devDependencies" : {
    "grunt-concurrent"         : "~1.0.0",
    "grunt-contrib-csslint"    : "^0.3.1",
    "grunt-contrib-cssmin"     : "~0.10.0",
    "grunt-contrib-jshint"     : "~0.10.0",
    "grunt-contrib-uglify"     : "~0.6.0",
    "grunt-contrib-watch"      : "~0.6.1",
    "grunt-env"                : "~0.4.1",
    "grunt-karma"              : "~0.9.0",
    "grunt-mocha-test"         : "~0.12.1",
    "grunt-ng-annotate"        : "~0.4.0",
    "grunt-node-inspector"     : "~0.1.3",
    "grunt-nodemon"            : "~0.3.0",
    "grunt-prompt"             : "^1.3.0",
    "grunt-protractor-runner"  : "^1.1.4",
    "karma"                    : "~0.12.0",
    "karma-chrome-launcher"    : "~0.1.2",
    "karma-coverage"           : "~0.2.0",
    "karma-firefox-launcher"   : "~0.1.3",
    "karma-jasmine"            : "~0.2.1",
    "karma-phantomjs-launcher" : "~0.1.2",
    "load-grunt-tasks"         : "~0.6.0",
    "mysql"                    : "^2.5.3",
    "should"                   : "~4.0.4",
    "supertest"                : "~0.14.0"
  }
}, "watch"           : {
  "serverViews" : {
    "files"   : ["app/views/**/*.*"],
    "options" : {
      "livereload" : true
    }
  },
  "serverJS"    : {
    "files"   : ["gruntfile.js", "server.js", "config/**/*.js", "app/**/*.js"],
    "tasks"   : ["jshint"],
    "options" : {
      "livereload" : true
    }
  },
  "clientViews" : {
    "files"   : ["public/modules/**/views/**/*.html"],
    "options" : {
      "livereload" : true
    }
  },
  "clientJS"    : {
    "files"   : ["public/js/*.js", "public/modules/**/*.js"],
    "tasks"   : ["jshint"],
    "options" : {
      "livereload" : true
    }
  },
  "clientCSS"   : {
    "files"   : ["public/modules/**/*.css"],
    "tasks"   : ["csslint"],
    "options" : {
      "livereload" : true
    }
  },
  "lessFiles"   : {
    "files"   : ["public/modules/**/*.less"],
    "tasks"   : ["loadConfig", "less:dev"],
    "options" : {}
  },
  "spriteFiles" : {
    "files"   : ["public/modules/**/img/**/*.png"],
    "tasks"   : ["loadConfig", "spriteGenerator:sprite"],
    "options" : {
      "livereload" : true
    }
  }
}, "jshint"          : {
  "all" : {
    "src"     : ["public/js/*.js", "public/modules/**/*.js", "gruntfile.js", "server.js", "config/**/*.js", "app/**/*.js"],
    "options" : {
      "jshintrc" : true
    }
  }
}, "csslint"         : {
  "options" : {
    "csslintrc" : ".csslintrc"
  },
  "all"     : {
    "src" : ["public/modules/**/*.css"]
  }
}, "uglify"          : {
  "production" : {
    "options" : {
      "mangle" : false
    },
    "files"   : {
      "public/dist/application.min.js" : "public/dist/application.js"
    }
  }
}, "cssmin"          : {
  "combine" : {
    "files" : {
      "public/dist/application.min.css" : ""
    }
  }
}, "nodemon"         : {
  "dev" : {
    "script"  : "server.js",
    "options" : {
      "nodeArgs" : ["--debug"],
      "ext"      : "js,html",
      "ignore"   : ["node_modules/**", "public/**"],
      "watch"    : ["app/views/**/*.*", "gruntfile.js", "server.js", "config/**/*.js", "app/**/*.js"]
    }
  }
}, "node-inspector"  : {
  "custom" : {
    "options" : {
      "web-port"          : 1337,
      "web-host"          : "localhost",
      "debug-port"        : 5858,
      "save-live-edit"    : true,
      "no-preload"        : true,
      "stack-trace-limit" : 50,
      "hidden"            : []
    }
  }
}, "ngAnnotate"      : {
  "production" : {
    "files" : {
      "public/dist/application.js" : ""
    }
  }
}, "concurrent"      : {
  "default" : ["nodemon", "watch"],
  "debug"   : ["nodemon", "watch", "node-inspector"],
  "dev"     : ["nodemon", "watch:lessFiles"],
  "options" : {
    "logConcurrentOutput" : true,
    "limit"               : 10
  }
}, "env"             : {
  "test"       : {
    "NODE_ENV" : "test"
  },
  "production" : {
    "NODE_ENV" : "production"
  },
  "dev"        : {
    "NODE_ENV" : "development"
  }
}, "mochaTest"       : {
  "src"     : ["app/tests/**/*.js"],
  "options" : {
    "reporter" : "spec",
    "require"  : "server.js"
  }
}, "less"            : {
  "dev"        : {
    "options" : {
      "sourceMap"       : true,
      "dumpLineNumbers" : "comments",
      "paths"           : "",
      "relativeUrls"    : true
    },
    "files"   : {
      "public/dist/application.css" : ""
    }
  },
  "production" : {
    "options" : {
      "paths"    : "",
      "cleancss" : true
    },
    "files"   : {
      "public/dist/application.min.css" : ""
    }
  }
}, "spriteGenerator" : {
  "sprite" : {
    "src"            : [""],
    "spritePath"     : "",
    "stylesheetPath" : "",
    "stylesheet"     : "less"
  }
}, "protractor"      : {
  "options" : {
    "keepAlive" : true,
    "noColor"   : false
  },
  "dev"     : {
    "options" : {
      "configFile" : "protractor.conf.js"
    }
  }
}, "migrate"         : {
  "options" : {
    "mysqlUser" : "asd",
    "mysqlPass" : "c"
  }
}, "prompt"          : {
  "mysql" : {
    "options" : {
      "questions" : [
        {
          "config"  : "migrate.options.mysqlUser",
          "type"    : "input",
          "message" : "Enter mysql username: "
        },
        {
          "config"  : "migrate.options.mysqlPass",
          "type"    : "input",
          "message" : "Enter mysql password: "
        }
      ]
    }
  }
}}
